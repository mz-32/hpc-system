[server]
# 管理サーバーの設定を記述
host = "localhost"
# リソースモニターの表示用ポート
webport = "10089"
# ジョブ投下用ポート
socketport = "10088"
# ステータス取得インターバル
update_interval =  500

# 以下の項目を計算ノードの台数記述する
[[server.node]]
# 個々の計算サーバーの情報を記述
id = 1
# ホスト名(特に使用していない)
host = "ss01"
# 接続用IPアドレス
ip = "localhost"
# ステータス送受信用ポート
port = "55010"
